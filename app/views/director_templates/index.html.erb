<h1>List of all directors</h1>

<table border="1">
  <tr>
    <th>Id</th>
    <th>Image</th>
    <th>Name</th>
    <th>Dob</th>
    <th><th>
  </tr>
  <% Director.order({ dob: :desc}).each do |a_director| %>
    <tr>
      <td><%= a_director.id %></td>
      <td><img src="<%= a_director.image %>" alt="<%= a_director.name %>" style="width:250px"></img></td>
      <td><%= a_director.name %></td>
      <td><%= a_director.dob %></td>
      <td><a href="/directors/<%= a_director.id %>">Show More</a></td>
    </tr>
  <% end %>
</table>
